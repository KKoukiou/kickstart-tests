#test name: network-noipv4-httpks
# NOTE: httpks version (kickstart ifcfgs created in initramfs) has
# different criteria on RHEL (BOOTPROTO="none" vs no BOOTPROTO in
# Fedora)
url @KSTEST_URL@
install

network --device=ens4 --noipv4 --no-activate --onboot=yes
network --device=ens5 --noipv4 --activate --onboot=no

bootloader --timeout=1
zerombr
clearpart --all
autopart

keyboard us
lang en
timezone America/New_York
rootpw qweqwe
shutdown

%packages
%end

%post

@KSINCLUDE@ post-lib-network.sh

check_device_ifcfg_value ens4 BOOTPROTO none
check_device_ifcfg_value ens5 BOOTPROTO none

# No error was written to /root/RESULT file, everything is OK
if [[ ! -e /root/RESULT ]]; then
   echo SUCCESS > /root/RESULT
fi
%end
