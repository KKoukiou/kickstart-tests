#version=DEVEL
#test name: packages-default

%ksappend repos/default.ks
%ksappend common/common_no_payload.ks

%packages --default
%end

%post
rpm -q fedora-release
if [ $? -eq 0 ]; then
    echo SUCCESS > /root/RESULT
else
    echo '*** fedora-release package was not installed' > /root/RESULT
fi
%end
